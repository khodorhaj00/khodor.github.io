function setTextSize(){
//txt
    if(txt === undefined || txt ==="" || txt ===null){
        $("#header").attr('style',"font-size:12px");
    }else{
        $("#header").attr('style',"font-size:"+txt+"px");
    }
}

function setCTAposition(){
//setCTAposition
    if(ctafix === undefined || ctafix ==="" || ctafix ===null){
        $("#cta").attr('style',"bottom:20px");
    }else{
        $("#cta").attr('style',"bottom:"+ctafix+"px");
    }
}

function setAnimation(){
